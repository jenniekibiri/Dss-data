<!-- normal case -->
<product type="candy" name="Reeses">
    <retailPrice>1.99</retailPrice>
    <margin>0.50</margin>
  
  
  <week id="1">
      <qty>23</qty>
      <mkavg> 20</mkavg>
      <nos>98</nos>
  </week>
  
  <week id="2">
      <qty>33</qty>
